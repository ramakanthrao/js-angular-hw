{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "lib": ["es6","dom","es5"],
    "allowJs": true,
    "outDir": "target",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": false,
    "experimentalDecorators":true,
    "moduleResolution": "node"
  },
  "include": [
        "./src/",
        "./src/index.html"
    ]
}
